@import "./stylesheet.wxss";
page{
  height: 100%;
  width: 100%;
  background-color: rgba(255,222,173,.2);
}

.content{
  width: 100%;
  position: relative;
  height: 100%;
  display: flex;
  flex-direction:column;
}

swiper{
  margin:30rpx 50rpx 30rpx 50rpx;
  width: 100%;
}
swiper image{
  height: 100%;
  width: 87%;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, .9);
  border-radius:5%;
}

.item_page{
  background-color: rgb(241, 185, 185);
  height: 0%;
  display: flex;
  flex-direction:row;
  align-items:stretch;
  justify-content:center;
  flex-wrap:wrap;
}

.item{
  margin: 30rpx;
  flex-direction: row;
  /* align-items: center; */
  /* margin-bottom: 10rpx; */
  margin-top: 100rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  color: #4A4A4A;
  width: 280rpx;
  height: 280rpx;
  font-weight: 800;
  background-color: #FFFFF0;
  border-radius: 30rpx;
  box-shadow: 0 0 30rpx rgba(0, 0, 0, .8); 
}

.iconfont{
  text-align:center; 
  width: 100%;
  font-size: 150rpx;
  color: #4F4F4F;
  font-weight: 600;
  margin-top: 50rpx;
}
